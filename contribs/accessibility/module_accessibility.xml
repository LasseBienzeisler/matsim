      <classpath refid="accessibility.module.classpath"/>
      <src refid="accessibility.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.accessibility"/>
    </javac2>
    
    <copy todir="${accessibility.testoutput.dir}">
      <fileset dir="${module.accessibility.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.accessibility" description="cleanup module">
    <delete dir="${accessibility.output.dir}"/>
    <delete dir="${accessibility.testoutput.dir}"/>
  </target>
