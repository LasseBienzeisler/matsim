    <mkdir dir="${accessibility.output.dir}"/>
    <javac2 destdir="${accessibility.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.accessibility}/javac">
      <compilerarg line="${compiler.args.accessibility}"/>
      <bootclasspath refid="accessibility.module.bootclasspath"/>
      <classpath refid="accessibility.module.production.classpath"/>
      <src refid="accessibility.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.accessibility"/>
    </javac2>
    
    <copy todir="${accessibility.output.dir}">
      <fileset dir="${module.accessibility.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.accessibility.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.accessibility.tests" depends="register.custom.compilers,compile.module.accessibility.production" description="compile module accessibility; test classes" unless="skip.tests">
    <mkdir dir="${accessibility.testoutput.dir}"/>
    <javac2 destdir="${accessibility.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.accessibility}/javac">
