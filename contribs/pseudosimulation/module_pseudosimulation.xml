  <target name="compile.module.pseudosimulation.production" depends="register.custom.compilers,compile.module.common,compile.module.eventsbasedptrouter,compile.module.matsim-examples,compile.module.matsim" description="Compile module pseudosimulation; production classes">
    <mkdir dir="${pseudosimulation.output.dir}"/>
    <javac2 destdir="${pseudosimulation.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.pseudosimulation}/javac">
      <compilerarg line="${compiler.args.pseudosimulation}"/>
      <bootclasspath refid="pseudosimulation.module.bootclasspath"/>
      <classpath refid="pseudosimulation.module.production.classpath"/>
      <src refid="pseudosimulation.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.pseudosimulation"/>
    </javac2>
    
    <copy todir="${pseudosimulation.output.dir}">
      <fileset dir="${module.pseudosimulation.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.pseudosimulation.tests" depends="register.custom.compilers,compile.module.pseudosimulation.production" description="compile module pseudosimulation; test classes" unless="skip.tests">
    <mkdir dir="${pseudosimulation.testoutput.dir}"/>
    <javac2 destdir="${pseudosimulation.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.pseudosimulation}/javac">
      <compilerarg line="${compiler.args.pseudosimulation}"/>
      <bootclasspath refid="pseudosimulation.module.bootclasspath"/>
      <classpath refid="pseudosimulation.module.classpath"/>
      <src refid="pseudosimulation.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.pseudosimulation"/>
    </javac2>
    
    <copy todir="${pseudosimulation.testoutput.dir}">
      <fileset dir="${module.pseudosimulation.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
