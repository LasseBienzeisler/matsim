  <patternset id="excluded.from.module.otfvis">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.otfvis">
    <patternset refid="excluded.from.module.otfvis"/>
  </patternset>
  
  <path id="otfvis.module.sourcepath">
    <dirset dir="${module.otfvis.basedir}">
      <include name="src/main/java"/>
      <include name="src/main/resources"/>
    </dirset>
  </path>
  
  <path id="otfvis.module.test.sourcepath">
    <dirset dir="${module.otfvis.basedir}">
      <include name="src/test/java"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.otfvis" depends="compile.module.otfvis.production,compile.module.otfvis.tests" description="Compile module otfvis"/>
  
  <target name="compile.module.otfvis.production" depends="register.custom.compilers,compile.module.matsim" description="Compile module otfvis; production classes">
    <mkdir dir="${otfvis.output.dir}"/>
    <javac2 destdir="${otfvis.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.otfvis}/javac">
      <compilerarg line="${compiler.args.otfvis}"/>
      <bootclasspath refid="otfvis.module.bootclasspath"/>
      <classpath refid="otfvis.module.production.classpath"/>
      <src refid="otfvis.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.otfvis"/>
    </javac2>
    
    <copy todir="${otfvis.output.dir}">
      <fileset dir="${module.otfvis.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.otfvis.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.otfvis.tests" depends="register.custom.compilers,compile.module.otfvis.production" description="compile module otfvis; test classes" unless="skip.tests">
    <mkdir dir="${otfvis.testoutput.dir}"/>
    <javac2 destdir="${otfvis.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.otfvis}/javac">
      <compilerarg line="${compiler.args.otfvis}"/>
      <bootclasspath refid="otfvis.module.bootclasspath"/>
      <classpath refid="otfvis.module.classpath"/>
      <src refid="otfvis.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.otfvis"/>
    </javac2>
    
    <copy todir="${otfvis.testoutput.dir}">
      <fileset dir="${module.otfvis.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.otfvis" description="cleanup module">
    <delete dir="${otfvis.output.dir}"/>
    <delete dir="${otfvis.testoutput.dir}"/>
  </target>
</project>