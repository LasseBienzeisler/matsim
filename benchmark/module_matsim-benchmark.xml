      <bootclasspath refid="matsim-benchmark.module.bootclasspath"/>
      <classpath refid="matsim-benchmark.module.production.classpath"/>
      <src refid="matsim-benchmark.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.matsim-benchmark"/>
    </javac2>
    
    <copy todir="${matsim-benchmark.output.dir}">
      <fileset dir="${module.matsim-benchmark.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.matsim-benchmark.tests" depends="register.custom.compilers,compile.module.matsim-benchmark.production" description="compile module matsim-benchmark; test classes" unless="skip.tests"/>
  
  <target name="clean.module.matsim-benchmark" description="cleanup module">
    <delete dir="${matsim-benchmark.output.dir}"/>
    <delete dir="${matsim-benchmark.testoutput.dir}"/>
